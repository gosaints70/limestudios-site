<!-- Header -->
<header class="main-header">
    <section class="description row row-tablet">
      {{#if page.headline}}
        <h1 class="text-headline">{{page.headline}}</h1>
        <br>
        <h2 class="text-subhead">{{page.subhead}}</h2>
      {{else}}
        <h1 class="text-headline">Hi, I'm <span class="text-bold">Liam Craver</span>.</h1>
        <br>
        <h2 class="text-subhead">{{site.tagline}}</h2>
      {{/if}}
    </section>
</header>
